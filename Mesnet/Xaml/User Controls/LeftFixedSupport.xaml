<UserControl x:Class="Mesnet.Xaml.User_Controls.LeftFixedSupport"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    mc:Ignorable="d" Height="27" Width="7">

    <UserControl.RenderTransform>
        <RotateTransform x:Name="rotateTransform" CenterX="7" CenterY="13"/>
    </UserControl.RenderTransform>

    <Grid Width="7" Height="27">
        <Polygon x:Name="p1" Points="7,0 7,27 5,27 5,0" Fill="Black"/>
        <Polygon x:Name="p2" Points="6,0 0,6 1,7 6,2.2" Fill="Black"/>
        <Polygon x:Name="p3" Points="6,5 0,11 1,12 6,7.2" Fill="Black"/>
        <Polygon x:Name="p4" Points="6,10 0,16 1,17 6,12.2" Fill="Black"/>
        <Polygon x:Name="p5" Points="6,15 0,21 1,22 6,17.2" Fill="Black"/>
        <Polygon x:Name="p6" Points="6,20 0,26 1,27 6,22.2" Fill="Black"/>
        <Polygon x:Name="core" Points="0,0 6,0 6,27 0,27 0,0" Fill="Transparent"/>
    </Grid>

</UserControl>
