<UserControl x:Class="Mesnet.Xaml.User_Controls.SlidingSupport"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" Height="21" Width="26">

    <UserControl.RenderTransform>
        <RotateTransform x:Name="rotateTransform" CenterX="13" CenterY="0"/>
    </UserControl.RenderTransform>

    <Grid Height="21" Width="26" RenderTransformOrigin="0.5,0.077">
        <Polygon x:Name="p1" Points="13,0 4,16 22,16 13,0 13,3 6.5,14.5 19.5,14.5 13,3" Fill="Black"/>
        <Ellipse x:Name="e1" Height="5" Width="5" Margin="6,16,15,0" Fill="Black"/>
        <Ellipse x:Name="e2" Height="5" Width="5" Margin="15,16,6,0" Fill="Black"/>
    </Grid>
</UserControl>

